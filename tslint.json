{
    "extends": "tslint:recommended",
    "rules": {
		"indent": [true, "tabs"],
		"quotemark": false,
		"ordered-imports": false,
		"member-access": false,
		"variable-name": false,
		"max-line-length": false,
		"object-literal-sort-keys": false,
		"trailing-comma": false,
		"one-line": [true, "check-open-brace", "check-whitespace"]
	}
}
